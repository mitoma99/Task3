<main>
 <h2>User</h2>

 <thead>
  <tr>
   <th>image</th>
   <th>name</th>
  </tr>
 </thead>

 <tbody>
  <% @users.each do |user| %>
  <tr>
   <td><%= image_tag user.get_profile_image(100,100) %></td>
   <td><%= user.name %></td>
   <td><%= link_to "Show", user_path(user) %></td>
  </tr>
  <% end %>
 </tbody>
